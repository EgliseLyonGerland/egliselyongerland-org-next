{
  "name": "egliselyongerland-org-next",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "lint": "next lint",
    "format": "prettier --write --ignore-unknown '**/*'",
    "format-check": "prettier --check --ignore-unknown '**/*'",
    "prepare": "husky install"
  },
  "dependencies": {
    "@apollo/client": "3.6.8",
    "graphql": "16.5.0",
    "next": "12.1.6",
    "react": "18.2.0",
    "react-dom": "18.2.0"
  },
  "devDependencies": {
    "@types/node": "18.0.0",
    "@types/react": "18.0.14",
    "@types/react-dom": "18.0.5",
    "@typescript-eslint/eslint-plugin": "5.29.0",
    "eslint": "8.18.0",
    "eslint-config-next": "12.1.6",
    "husky": "8.0.1",
    "lint-staged": "13.0.2",
    "prettier": "2.7.1",
    "typescript": "4.7.4"
  },
  "lint-staged": {
    "*.{js,jsx,ts,tsx}": [
      "eslint --fix --max-warnings=0"
    ],
    "*": [
      "prettier --write --ignore-unknown"
    ]
  }
